class Node
    v = none
    next = none

    fun _init (this, a)
        this.v = a

    fun print (this)
        put ("Node value: ")
        putln (this.v)

a = 1 to 10
cur = dummy = Node (0)

for i in a
    cur.next = Node (i)
    cur = cur.next

dummy = dummy.next
while dummy != none
    dummy.print()
    dummy = dummy.next