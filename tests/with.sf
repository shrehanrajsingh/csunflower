class Animal
    name = none
    life = none

    fun _init (self, name)
        self.name = name

class Dog (Animal)
    
    fun _init (self)
        with super (self, Animal)
            ._init ('dog')
            .life = 15
    
    fun speak (self)
        putln ('Bark')
    
    fun getlife (self)
        putln (self.life)

d = Dog ()
d.speak ()